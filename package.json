{
  "name": "czyste-pragnienia-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy-prod": "git push heroku master",
    "lint": "next lint",
    "download-schema": "apollo service:download --endpoint=http://localhost:4000/api/graphql graphql-schema.json",
    "codegen": "npm run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json,client-schema.graphql --target=typescript --tagName=gql"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@apollo/link-error": "^2.0.0-beta.3",
    "apollo-upload-client": "^17.0.0",
    "next": "12.1.6",
    "next-with-apollo": "^5.3.0",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^17.0.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "apollo": "^2.33.11",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.6.4"
  }
}
